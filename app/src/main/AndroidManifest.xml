<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.usurio.myapplication">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:debuggable="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
                </intent-filter>
        </activity>

        <activity android:name=".AmigosActivity" />
        <activity android:name=".AvaliacaoActivity" />
        <activity android:name=".BoxUsersActivity" />
        <activity android:name=".CentrosActivity" />
        <activity android:name=".CorpoAdminActivity" />
        <activity android:name=".CadastroActivity" />
        <activity android:name=".EditarConteudoActivity" />
        <activity android:name=".EscolhaLeituraActivity" />
        <activity android:name=".EventosActivity" />
        <activity android:name=".GuiaActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".OpcoesActivity" />
        <activity android:name=".TelaAdminActivity" />
        <activity android:name=".TelaUsuarioActivity" />

    </application>

</manifest>

