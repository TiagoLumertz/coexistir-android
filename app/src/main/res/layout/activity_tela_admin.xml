<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:background="#5CB1F3"
    tools:context="com.example.usurio.myapplication.MainActivity">

    <ImageButton
        android:id="@+id/ib_ed_conteudos"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/edit"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="205dp" />

    <ImageButton
        android:id="@+id/ib_corp_admin"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/id"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="205dp" />

    <ImageButton
        android:id="@+id/ib_avaliacoes"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/file"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="205dp" />

    <ImageButton
        android:id="@+id/ib_opcoes_admin"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/settings"
        tools:layout_editor_absoluteX="318dp"
        tools:layout_editor_absoluteY="21dp" />

    <ImageButton
        android:id="@+id/ib_notificacao"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/alarm"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="21dp" />

    <TextView
        android:id="@+id/tv_ed_conteudos"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:text="Editar conteúdos"
        android:textAlignment="center"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/tv_avaliacoes"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:text="Consultar avaliações"
        android:textAlignment="center"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="142dp"
        tools:layout_editor_absoluteY="305dp" />


    <TextView
        android:id="@+id/tv_corp_admin"
        android:layout_width="100dp"
        android:layout_height="35dp"
        android:text="Corpo administrativo"
        android:textAlignment="center"
        android:textColor="#ffffff"
        tools:layout_editor_absoluteX="257dp"
        tools:layout_editor_absoluteY="305dp" />

    <TextView
        android:id="@+id/tv_ola"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Olá, Administrador!"
        android:textColor="#ffffff"
        android:textSize="25dp"
        tools:layout_editor_absoluteX="24dp"
        tools:layout_editor_absoluteY="32dp" />

</android.support.constraint.ConstraintLayout>